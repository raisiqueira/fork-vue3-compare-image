{
  "name": "vue3-compare-image",
  "version": "1.1.1",
  "packageManager": "pnpm@8.6.5",
  "description": "A Vue 3 component to compare and slide between two images, supports vertical and horizontal modes, and Keyboard for accessibility.",
  "author": "Cedrouseroll Omondi <omondicedo@gmail.com>",
  "license": "MIT",
  "readme": "./README.md",
  "homepage": "https://github.com/zedjarvis/vue3-compare-image#readme",
  "bugs": "https://github.com/zedjarvis/vue3-compare-image/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zedjarvis/vue3-compare-image"
  },
  "keywords": [
    "vue3 compare image",
    "picture comparison",
    "image comparison",
    "compare image",
    "image slider",
    "image diff",
    "latest",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js",
      "import": "./dist/components/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@types/node": "^20.4.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.4.0",
    "eslint": "^8.45.0",
    "typescript": "^5.1.6",
    "unbuild": "2.0.0-rc.0",
    "vite": "^4.4.6",
    "vue": "^3.3.4"
  }
}